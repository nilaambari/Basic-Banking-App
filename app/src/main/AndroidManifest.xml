<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.basic_banking_application">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.basic_banking_application.activity_account" />
        <activity android:name="com.example.basic_banking_application.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.basic_banking_application.AccountDetails"/>
        <activity android:name="com.example.basic_banking_application.receiver_list"/>
        <activity android:name="com.example.basic_banking_application.money_transfer"/>
    </application>

</manifest>
